{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "db:generate"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "db:deploy": {},
    "db:reset": {},
    "db:push": {},
    "db:seed": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^dev", "db:generate"]
    },
    "db:generate": {
      "cache": false,
      "dependsOn": ["^db:generate"]
    },
    "lint": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^test", "db:generate"]
    }
  }
}
